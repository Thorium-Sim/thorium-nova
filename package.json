{
  "name": "thorium-nova",
  "version": "0.0.1",
  "description": "Next-Generation Spaceship Controls",
  "main": "dist/index.html",
  "targets": {
    "worker": {
      "distDir": "./dist"
    },
    "app": {
      "distDir": "./dist"
    }
  },
  "repository": "https://github.com/thorium-sim/thorium-next",
  "author": "Alex Anderson (alex@fyreworks.us)",
  "license": "MIT",
  "engines": {
    "node": "^14"
  },
  "scripts": {
    "start": "npm-run-all --parallel start:client start:server",
    "start:client": "NODE_ENV=development node scripts/start.js",
    "start:server": "ts-node-dev --trace-deprecation --trace-warnings -r tsconfig-paths/register server/index.ts",
    "build": "npm-run-all build:client",
    "build:client": "node scripts/build.js",
    "prebuild:client": "NODE_ENV=production npm run generate:tailwind",
    "postbuild:client": "node scripts/copy-static.mjs",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "generate:graphql": "graphql-codegen --config codegen.yml",
    "generate:schema": "npx get-graphql-schema http://localhost:3001/graphql > schema.graphql",
    "generate:tailwind": "npx tailwindcss-cli@latest build  client/styles/tailwind.css -o client/styles/output.css",
    "postgenerate:graphql": "prettier --write client/generated/graphql.tsx",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.+(js|jsx|yml|yaml|css|less|scss|ts|tsx|md|mdx|graphql|vue)\"",
    "format:different": "prettier --list-different \"**/*.+(js|jsx|yml|yaml|css|less|scss|ts|tsx|md|mdx|graphql|vue)\""
  },
  "browserslist": [
    "maintained node versions",
    "last 1 chrome version",
    "last 1 firefox version",
    "last 1 safari version"
  ],
  "alias": {
    "client": "./client"
  },
  "proxy": "http://localhost:3001",
  "madge": {
    "fontSize": "10px",
    "graphVizOptions": {
      "G": {
        "rankdir": "LR"
      }
    },
    "tsConfig": "tsconfig.json",
    "fileExtensions": [
      "ts",
      "js",
      "tsx"
    ],
    "detectiveOptions": {
      "ts": {
        "skipTypeImports": true
      }
    }
  },
