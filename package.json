{
  "name": "thorium-next",
  "version": "0.0.1",
  "description": "Next-Generation Spaceship Controls",
  "main": "index.js",
  "repository": "https://github.com/thorium-sim/thorium-next",
  "author": "Alex Anderson (alex@fyreworks.us)",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": "^14"
  },
  "scripts": {
    "start:client": "NODE_ENV=development parcel serve client/index.html --port 3000 --open",
    "start:server": "ts-node-dev -r tsconfig-paths/register server/index.ts",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "generate:graphql": "graphql-codegen --config codegen.yml",
    "postgenerate:graphql": "npm run format",
    "format": "prettier --write \"**/*.+(js|jsx|yml|yaml|css|less|scss|ts|tsx|md|mdx|graphql|vue)\""
  },
  "browserslist": [
    "maintained node versions",
    "last 1 chrome version",
    "last 1 firefox version",
    "last 1 safari version"
  ],
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "apollo-server-express": "^2.15.0",
    "babel-plugin-transform-node-env-inline": "^0.4.3",
    "bonjour": "^3.5.0",
    "class-validator": "^0.12.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "ecsy": "MozillaReality/ecsy#dev",
    "emotion-theming": "^10.0.27",
    "express": "^4.16.4",
    "graphql": "^14.0.0 || ^15.0.0",
    "lodash.throttle": "^4.1.1",
    "multer": "^1.4.2",
    "prando": "^5.1.2",
    "random-words": "^1.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-error-boundary": "^2.2.2",
    "react-use-konami": "^1.0.13",
    "reflect-metadata": "^0.1.13",
    "subscriptions-transport-ws": "^0.9.0",
    "ts-node-dev": "^1.0.0-pre.49",
    "type-graphql": "^1.0.0-rc.2",
    "uniqid": "^5.2.0",
    "zustand": "^2.2.3"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.10.3",
    "@babel/plugin-transform-typescript": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "^7.10.1",
    "@babel/preset-typescript": "^7.10.1",
    "@babel/runtime": "^7.10.3",
    "@emotion/babel-preset-css-prop": "^10.0.27",
    "@graphql-codegen/cli": "1.15.4",
    "@graphql-codegen/typescript": "1.15.4",
    "@graphql-codegen/typescript-operations": "1.15.4",
    "@testing-library/jest-dom": "^5.10.1",
    "@testing-library/react": "^10.4.1",
    "@testing-library/user-event": "^12.0.7",
    "@types/bonjour": "^3.5.6",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.1",
    "@types/jest": "^26.0.2",
    "@types/lodash.throttle": "^4.1.6",
    "@types/multer": "^1.4.3",
    "@types/react": "^16.9.38",
    "@types/react-dom": "^16.9.8",
    "@types/uniqid": "^5.2.0",
    "babel-plugin-macros": "^2.8.0",
    "eslint-import-resolver-jest": "^3.0.0",
    "eslint-import-resolver-parcel": "^1.10.4",
    "eslint-import-resolver-typescript": "^2.0.0",
    "jest": "^26.1.0",
    "jest-emotion": "^10.0.32",
